<script>
    Vue.component('list-component', {
        template: '#list-component-template',
        props: {
            item: {}
        },
        methods: {
            //点赞
            clickPraise: function (item) {
                var _this = this;
                var reqData = {
                    main_id: item.id
                };
                if (_this.isLoading) return;
                _this.isLoading = true;
                $.ajax({
                    type: 'post',
                    url: '/Relation/AddRelation',
                    data: reqData,
                    dataType: 'json',
                    success: function (resp) {
                        _this.isLoading = false;
                        if (resp.status) {
                            item.is_praise = true;
                            item.praise_num++;

                        } else {
                            alert(resp.msg);
                        }
                    }
                });
            },
            //取消点赞
            cancelClickPraise: function (item) {
                var _this = this;
                var reqData = {
                    main_id: item.id
                };
                if (_this.isLoading) return;
                _this.isLoading = true;

                $.ajax({
                    type: 'post',
                    url: '/Relation/DeleteRelation',
                    data: reqData,
                    dataType: 'json',
                    success: function (resp) {
                        _this.isLoading = false;
                        if (resp.status) {
                            item.is_praise = false;
                            item.praise_num--;

                        } else {
                            alert(resp.msg);
                        }
                    }
                });
            }
        }






    });
    </script>