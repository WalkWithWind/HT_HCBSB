
@{
    ViewBag.Title = "我的团队——二级分销";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="teamChild">
    <div class="top">
        <div class="wrapper">
            <a href="javascript:history.go(-1);" class="back_btn">返回</a>
        </div>
    </div>
    <ul class="tm_sec2 sd_sec">


        <li v-for="item in teamChildData.list">
            <div class="pt1">
                <img v-bind:src="item.source_user_avatar">
            </div>
            <div class="pt2">
                <span v-text="item.source_user_nick"></span>
                <p>加入时间：{{item.add_time|jsonFormart}}</p>
            </div>
            <div class="pt3">
                <span>分销收益</span>
                <em>￥{{item.total_money.toFixed(2)}}</em>
            </div>
        </li>


        <li v-show="teamChildData.list.length==0" class="nodata">
            暂无记录
        </li>

    </ul>
</div>

@Scripts.Render("/Scripts/User/SecondaryDistribution.js")
